{
  "name": "weekly-percentage-tracker",
  "version": "1.0.0",
  "description": "A web application to track weekly time allocation across different projects",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build:css": "npx --package=tailwindcss tailwindcss -i ./src/input.css -o ./build/style.css --minify",
    "build": "npm run build:css && cp index.html build/ && cp index.js build/ && cp utils.js build/ && cp -r data build/ && echo 'Build completed successfully'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "timesheet",
    "tracker",
    "percentage",
    "weekly"
  ],
